{
  "name": "ng-maps",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:google": "nx serve sample",
    "start:here": "nx serve sample-here",
    "build": "nx build",
    "build:core": "nx build core",
    "build:drawing-layer": "nx build drawing-layer",
    "build:google": "nx build google",
    "build:here": "nx build here",
    "build:marker-clusterer": "nx build marker-clusterer",
    "build:places": "nx build places",
    "build:snazzy-info-window": "nx build snazzy-info-window",
    "build:all": "npm run build:core && npm run build:google && npm run build:here && npm run build:drawing-layer && npm run build:marker-clusterer && npm run build:places && npm run build:snazzy-info-window",
    "test": "nx test",
    "test:ci": "nx test core -c ci",
    "test:drawing-layer": "nx test drawing-layer",
    "test:ci:drawing-layer": "nx test drawing-layer -c ci",
    "test:google": "nx test google",
    "test:ci:google": "nx test google -c ci",
    "test:here": "nx test here",
    "test:ci:here": "nx test here -c ci",
    "test:marker-clusterer": "nx test marker-clusterer",
    "test:ci:marker-clusterer": "nx test marker-clusterer -c ci",
    "test:places": "nx test places",
    "test:ci:places": "nx test places -c ci",
    "test:snazzy-info-window": "nx test snazzy-info-window",
    "test:ci:snazzy-info-window": "nx test snazzy-info-window -c ci",
    "test:all-ci": "npm run test:ci && npm run test:ci:drawing-layer && npm run test:ci:marker-clusterer",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "doc:core": "cd libs/core && npx compodoc",
    "doc:google": "cd libs/google && npx compodoc",
    "doc:here": "cd libs/here && npx compodoc",
    "doc:drawing-layer": "cd libs/drawing-layer && npx compodoc",
    "doc:marker-clusterer": "cd libs/marker-clusterer && npx compodoc",
    "doc:places": "cd libs/places && npx compodoc",
    "doc:all": "npm run doc:core && npm run doc:google && npm run doc:here && npm run doc:drawing-layer && npm run doc:marker-clusterer && npm run doc:places",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "projects/core/**/*.ts": [
      "ng-lint-staged lint core --force --fix --",
      "git add"
    ],
    "projects/google/**/*.ts": [
      "ng-lint-staged lint google --force --fix --",
      "git add"
    ],
    "projects/here/**/*.ts": [
      "ng-lint-staged lint here --force --fix --",
      "git add"
    ],
    "projects/marker-clusterer/**/*.ts": [
      "ng-lint-staged lint marker-clusterer --force --fix --",
      "git add"
    ],
    "projects/places/**/*.ts": [
      "ng-lint-staged lint places --force --fix --",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "@googlemaps/markerclusterer": "^2.0.0",
    "@nrwl/angular": "15.3.3",
    "rxjs": "^7.6.0",
    "snazzy-info-window": "^1.1.1",
    "tslib": "^2.3.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.4",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.4",
    "@angular/language-service": "15.0.4",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/config-nx-scopes": "^17.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@jscutlery/semver": "^2.29.2",
    "@ngneat/spectator": "^12.2.1",
    "@nrwl/eslint-plugin-nx": "15.3.3",
    "@nrwl/linter": "15.3.3",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.3.3",
    "@twittwer/compodoc": "^1.9.0",
    "@types/google.maps": "~3.51.0",
    "@types/heremaps": "github:ng-maps/heremaps-typings",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^9.2.1",
    "ng-lint-staged": "^0.1.6",
    "ng-mocks": "^14.5.0",
    "ng-packagr": "15.0.3",
    "ngx-deploy-npm": "^4.3.10",
    "nx": "15.3.3",
    "postcss": "^8.4.20",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.8.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.0",
    "typescript": "4.8.4"
  }
}
