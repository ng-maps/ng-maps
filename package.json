{
  "name": "ng-maps",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:google": "nx serve sample",
    "start:here": "nx serve sample-here",
    "build": "nx build",
    "build:core": "nx build core",
    "build:drawing-layer": "nx build drawing-layer",
    "build:google": "nx build google",
    "build:here": "nx build here",
    "build:marker-clusterer": "nx build marker-clusterer",
    "build:places": "nx build places",
    "build:snazzy-info-window": "nx build snazzy-info-window",
    "build:direction": "nx build direction",
    "build:all": "npm run build:core && npm run build:google && npm run build:here && npm run build:drawing-layer && npm run build:marker-clusterer && npm run build:places && npm run build:snazzy-info-window && npm run build:direction",
    "build:all:production": "nx run-many --all --target=build --configuration=production",
    "test": "nx test",
    "test:ci": "nx test core -c ci",
    "test:drawing-layer": "nx test drawing-layer",
    "test:ci:drawing-layer": "nx test drawing-layer -c ci",
    "test:google": "nx test google",
    "test:ci:google": "nx test google -c ci",
    "test:here": "nx test here",
    "test:ci:here": "nx test here -c ci",
    "test:marker-clusterer": "nx test marker-clusterer",
    "test:ci:marker-clusterer": "nx test marker-clusterer -c ci",
    "test:places": "nx test places",
    "test:ci:places": "nx test places -c ci",
    "test:snazzy-info-window": "nx test snazzy-info-window",
    "test:ci:snazzy-info-window": "nx test snazzy-info-window -c ci",
    "test:direction": "nx test direction",
    "test:ci:direction": "nx test direction -c ci",
    "test:all-ci": "npm run test:ci && npm run test:ci:drawing-layer && npm run test:ci:marker-clusterer && npm run test:ci:direction",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "doc:core": "cd libs/core && npx compodoc",
    "doc:google": "cd libs/google && npx compodoc",
    "doc:here": "cd libs/here && npx compodoc",
    "doc:drawing-layer": "cd libs/drawing-layer && npx compodoc",
    "doc:marker-clusterer": "cd libs/marker-clusterer && npx compodoc",
    "doc:places": "cd libs/places && npx compodoc",
    "doc:direction": "cd libs/direction && npx compodoc",
    "doc:all": "npm run doc:core && npm run doc:google && npm run doc:here && npm run doc:drawing-layer && npm run doc:marker-clusterer && npm run doc:places && npm run doc:direction",
    "prepare": "husky install",
    "release": "nx run-many --target=version --parallel=1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "projects/core/**/*.ts": [
      "ng-lint-staged lint core --force --fix --",
      "git add"
    ],
    "projects/google/**/*.ts": [
      "ng-lint-staged lint google --force --fix --",
      "git add"
    ],
    "projects/here/**/*.ts": [
      "ng-lint-staged lint here --force --fix --",
      "git add"
    ],
    "projects/marker-clusterer/**/*.ts": [
      "ng-lint-staged lint marker-clusterer --force --fix --",
      "git add"
    ],
    "projects/places/**/*.ts": [
      "ng-lint-staged lint places --force --fix --",
      "git add"
    ],
    "projects/direction/**/*.ts": [
      "ng-lint-staged lint direction --force --fix --",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.2",
    "@angular/common": "16.0.2",
    "@angular/compiler": "16.0.2",
    "@angular/core": "16.0.2",
    "@angular/forms": "16.0.2",
    "@angular/platform-browser": "16.0.2",
    "@angular/platform-browser-dynamic": "16.0.2",
    "@angular/router": "16.0.2",
    "@googlemaps/markerclusterer": "^2.0.0",
    "@nx/angular": "16.3.2",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.6.0",
    "snazzy-info-window": "^1.1.1",
    "tslib": "^2.3.1",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.5",
    "@angular-devkit/core": "16.0.2",
    "@angular-devkit/schematics": "16.0.2",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.0.2",
    "@angular/compiler-cli": "16.0.2",
    "@angular/language-service": "16.0.2",
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@commitlint/config-nx-scopes": "17.6.4",
    "@compodoc/compodoc": "^1.1.11",
    "@jscutlery/semver": "3.0.0",
    "@ngneat/spectator": "^15.0.0",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/linter": "16.3.2",
    "@nx/workspace": "16.3.2",
    "@schematics/angular": "16.0.2",
    "@twittwer/compodoc": "1.9.0",
    "@types/google.maps": "~3.53.0",
    "@types/heremaps": "github:ng-maps/heremaps-typings",
    "@types/jasmine": "^4.3.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.0.0",
    "@types/snazzy-info-window": "^1.1.7",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.2",
    "jasmine-core": "~3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^9.2.1",
    "ng-lint-staged": "^12.0.0",
    "ng-mocks": "14.10.1",
    "ng-packagr": "16.0.1",
    "ngx-deploy-npm": "^6.0.0",
    "nx": "16.3.2",
    "nx-cloud": "16.0.5",
    "postcss": "^8.4.20",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~8.4.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
